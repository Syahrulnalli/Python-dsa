   "source": [
    "#factorial of 6 \n",
    "def factorial(num):\n",
    "   if num == 0:\n",
    "       return 1\n",
    "   else:\n",
    "       return num*factorial(num-1)\n",
    "n = int(input(\"Enter any number: \"))\n",
    "fact = factorial(n)\n",
    "print(\"Factorial output:\",fact)"
   ]
